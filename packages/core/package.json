{
  "name": "@codelet/core",
  "version": "0.0.1",
  "description": "mini core",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "release": "npm run build && dsc publish --pnpm"
  },
  "exports": {
    ".": "./src/index.ts",
    "./plugins": "./src/plugins/index.ts",
    "./plugins/*": "./src/plugins/*",
    "./*": "./src/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./plugins": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      },
      "./plugins/*": "./dist/plugins/*",
      "./*": "./dist/*",
      "./package.json": "./package.json"
    },
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "daysnap",
    "codelet",
    "wechat",
    "miniprogram",
    "core",
    "mini"
  ],
  "author": "woshiajuana",
  "license": "MIT",
  "dependencies": {
    "@daysnap/types": "^0.0.5",
    "@daysnap/utils": "^0.0.85",
    "miniprogram-api-typings": "^3.12.2"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0"
  }
}
